<tr>
  <td class="truncate"><%= w000t.long_url %></td>
  <td><%= w000t.number_of_click %></td>
  <td>
    <%= pretty_http_status(w000t.url_info) %>
  </td>
  <td>
    <%= link_to redirect_path(w000t), method: :get, class: 'btn btn-default' do  %>
      <% url_type_link(w000t.url_info) %>
    <% end %>
    <%= link_to w000t, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-default' do %>
      <i class="fa fa-trash"></i>
    <% end %>
    <% if current_user && current_user.admin? %>
      <%= link_to admin_check_url_path(admin: { long_url: w000t.long_url }),
          method: :post, class: 'btn btn-default' do  %>
        <i class="fa fa-link"></i>
      <% end %>
    <% end %>
  </td>
</tr>
